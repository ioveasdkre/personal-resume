{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint -c ./eslint.config.mjs \"./src*/**/*.{mjs,ts,vue}\"",
    "format": "prettier --write --ignore-unknown \"**/*.{mjs,ts,vue,md,json}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/eslint": "1.7.1",
    "eslint": "^9.32.0",
    "motion-v": "^1.6.1",
    "nuxt": "^4.0.2",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxtjs/i18n": "^10.0.3",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nuxt-security": "^2.3.0",
    "nuxt-swiper": "^2.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.8.3",
    "vue-tsc": "^3.0.4"
  },
  "lint-staged": {
    "*.{ts,mjs,vue}": [
      "eslint --fix",
      "prettier --write . --ignore-unknown"
    ]
  }
}
